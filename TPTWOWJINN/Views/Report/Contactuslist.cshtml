
@{
    ViewBag.Title = "Contactuslist";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@using System.Data;
@model MDYOS.Models.PropertyClass

<div class="main-body p-0">
    <div class="inner-wrapper">
        <!-- Inner sidebar -->
        <div class="inner-sidebar">
            <!-- Inner sidebar header -->
            <div class="inner-sidebar-header justify-content-center">
                @*<button class="btn btn-primary has-icon btn-block" type="button" data-toggle="modal" data-target="#threadModal">
                        <i data-feather="plus" class="mr-2"></i> NEW DISCUSSION
                    </button>*@

            </div>
            <!-- /Inner sidebar header -->
            <!-- Inner sidebar body -->
            <div class="inner-sidebar-body p-0">
                <div class="p-3 h-100" data-simplebar>
                    <nav class="nav nav-pills nav-gap-y-1 flex-column">
                        <a href="../Customer/Custmerlist" class="nav-link nav-link-faded has-icon"><i class="material-icons mr-2">verified_user</i>Customer's List</a>
                        <a href="../Retailers/AllRetailers" class="nav-link nav-link-faded has-icon active"><i class="material-icons mr-2">view_module</i>Enquiry</a>
                        @*       <a href="../Retailers/ActiveRetailersAccounts" class="nav-link nav-link-faded has-icon"><i class="material-icons mr-2">check_circle_outline</i>Active Accounts</a>
                            <a href="../Retailers/BlockedRetailersAccount" class="nav-link nav-link-faded has-icon"><i class="material-icons mr-2">highlight_off</i>Blocked Accounts</a>*@
                    </nav>
                </div>
            </div>
            <!-- /Inner sidebar body -->
        </div>
        <!-- /Inner sidebar -->
        <!-- Inner main -->
        <div class="inner-main">
            <!-- Inner main header -->
            @*<div class="inner-main-header">
                    <a class="nav-link nav-icon rounded-circle nav-link-faded mr-3 d-md-none" href="#" data-toggle="inner-sidebar"><i class="material-icons">arrow_forward_ios</i></a>

                    <span style="font-weight: bold;color: #f77e2f;font-size: 25px;">Manage Retailer Accounts</span>
                </div>*@
            <!-- /Inner main header -->
            <!-- Inner main body -->
            <!-- Forum List -->
            <div class="inner-main-body p-2 p-sm-3 collapse forum-content show">
                <section id="section3">
                    <div class="card">
                        <div class="card-body">
                            <div class="alert alert-accent alert-success" role="alert">
                                <h4 class="alert-heading">All Enquiry List</h4>
                                <hr>
                                <p class="mb-0">Here is all Enquiry .</p>
                            </div>
                            <table id="example" class="table table-striped table-bordered table-sm dt-responsive nowrap w-100">
                                <!-- Filter columns -->
                                <thead>
                                    @*<tr class="column-filter dt-column-filter">
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search First name"></th>
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search Last name"></th>
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search Position"></th>
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search Office"></th>
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search Age"></th>
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search Start date"></th>
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search Salary"></th>
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search Extn."></th>
                                            <th><input type="text" class="form-control form-control-sm" placeholder="Search E-mail"></th>
                                        </tr>*@
                                    <tr>
                                        <th>Sl No.</th>
                                        <th>Name</th>
                                        <th>Email Address</th>
                                        <th>Mobile Number</th>
                                        <th>Comments</th>
                                        <th>Enquiry Date</th>
                                    </tr>
                                </thead>
                                <!-- /Filter columns -->
                                <tbody>
                                    @if (Model.dt != null && Model.dt.Rows.Count > 0)
                                    {
                                        int i = 0;
                                        foreach (DataRow item in Model.dt.Rows)
                                        {
                                            i++;
                                            <tr>
                                                <td>@i</td>
                                                <td>@item["NAME"]</td>
                                                <td>@item["EMAILADDRESS"]</td>
                                                <td>@item["TITLE"]</td>
                                                <td>@item["COMMENTS"]</td>
                                                <td>@item["ENTRYDATE"]</td>
                                            </tr>
                                        }
                                    }


                                </tbody>
                            </table>
                        </div>
                    </div>
                </section>
            </div>
            <!-- Inner main -->
            <div class="inner-main collapse transition-none mail-content">
                <!-- Inner main header -->
                <div class="inner-main-header">
                    <button class="btn btn-danger btn-sm has-icon" data-toggle="collapse" data-target=".mail-content"><i data-feather="chevron-left"></i>Back to List</button>
                    <div class="btn-group btn-group-sm ml-2" role="group">
                        <button type="button" class="btn has-icon text-success" data-toggle="tooltip" data-trigger="hover" title="Activate Account"><i data-feather="archive"></i></button>
                        <button type="button" class="btn has-icon text-info" data-toggle="tooltip" data-trigger="hover" title="Suspend Account"><i data-feather="alert-octagon"></i></button>
                        @*<button type="button" class="btn has-icon text-danger" data-toggle="tooltip" data-trigger="hover" title="Delete"><i data-feather="trash"></i></button>*@

                        <input type="hidden" id="hdRetCode" />
                    </div>
                </div>
                <!-- /Inner main header -->
                <!-- Inner main body -->
                <div class="inner-main-body p-0 bg-white">
                    <div class="card rounded-0">
                        <div class="card-body font-size-sm">
                            <div class="media mb-3 align-items-center">
                                <img src="../AdminAsset/img/user.svg" alt="User" class="rounded" width="50" height="50">
                                <div class="media-body text-muted ml-3">
                                    <h6 class="mb-0 text-dark"><span id="spnBusinessName"></span></h6>
                                    <div class="small"><span id="spnRetId"></span> </div>
                                    <div class="small"><span id="spnCreatedOn"></span></div>
                                </div>
                                <div class="btn-group">

                                </div>
                            </div>
                            <h5>Review Retailer's Account</h5>
                            <hr>
                            <p>
                                <strong>Contact Name: </strong><span id="spnContactName"></span>
                            </p>
                            <p>
                                <strong>Email Address: </strong><span id="spnEmail"></span>
                            </p>
                            <p>
                                <strong>Contact No: </strong>+91-<span id="spnMobile"></span>
                            </p>
                            <p>
                                <strong>Business Address: </strong><span id="spnAddress"></span>
                            </p>
                            <h5>KYC Details</h5>
                            <hr>
                            <p>
                                <strong>KYC Document Type: </strong><span id="spnKYCDocType"></span>
                            </p>
                            <p>
                                <strong>KYC Document No: </strong><span id="spnKYCDocNo"></span>
                            </p>
                            <p>
                                <strong>KYC Document File: </strong><a id="kycDoc" target="_blank">View Document</a>
                            </p>
                            <p>
                                <strong>PAN No: </strong><span id="spnPanNo"></span>
                            </p>
                            <p>
                                <strong>PAN File: </strong><a id="PanDoc" target="_blank">View Document</a>
                            </p>
                            <p>
                                <strong>TAN No: </strong><span id="spnTanNo"></span>
                            </p>
                            <p>
                                <strong>TAN File: </strong><a id="TanDoc" target="_blank">View Document</a>
                            </p>
                            <h5>BANK Details</h5>
                            <hr>
                            <p>
                                <strong>Account Name: </strong><span id="spnAccName"></span>
                            </p>
                            <p>
                                <strong>Bank Name: </strong><span id="spnBankName"></span>
                            </p>
                            <p>
                                <strong>Account No: </strong><span id="spnAccountNo"></span>
                            </p>
                            <p>
                                <strong>IFSC Code: </strong><span id="spnIfscCode"></span>
                            </p>

                        </div>
                    </div>
                </div>
                <!-- /Inner main body -->
            </div>
            <!-- /Inner main -->


        </div>
        <!-- /Inner main -->
    </div>
    <!-- New Thread Modal -->
    <div class="modal fade" id="threadModal" tabindex="-1" role="dialog" aria-labelledby="threadModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <form>
                    <div class="modal-header d-flex align-items-center">
                        <h6 class="modal-title mb-0" id="threadModalLabel">New Discussion</h6>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            <label for="threadTitle">Title</label>
                            <input type="text" class="form-control" id="threadTitle" placeholder="Enter title" autofocus>
                        </div>
                        <textarea class="form-control summernote"></textarea>
                        <div class="custom-file form-control-sm mt-3" style="max-width: 300px">
                            <input type="file" class="custom-file-input" id="customFile" multiple>
                            <label class="custom-file-label" for="customFile">Attachment</label>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-light" data-dismiss="modal">Cancel</button>
                        <button type="button" class="btn btn-primary">Post</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

