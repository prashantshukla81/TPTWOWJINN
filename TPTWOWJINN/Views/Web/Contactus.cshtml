
@{
    ViewBag.Title = "Contactus";
    Layout = "~/Views/Shared/_webLayouts.cshtml";
}
<script src="~/assets/js/jquery-1.11.1.min.js"></script>
<script>
    function savedata() {

        var obj = {
            name: $('#txtname').val(),
            email: $('#txtemail').val(),
            mobile: $('#txtmobile').val(),
            comment: $('#txtcomment').val()
        }
        $.ajax({
            url: "@Url.Action("InsertContactus", "Web")",
            method: "post",
            data: JSON.stringify(obj),
            contentType: "application/json",
            success: function (r) {
                if (r.msg != null) {
                    alert(r.msg) 
                    $('#txtname').val('')
                    $('#txtemail').val('')
                    $('#txtmobile').val('')
                    $('#txtcomment').val('')
                }
            },
            error: function (r) {
                alert('Error');
            }
        })
    }
</script>

