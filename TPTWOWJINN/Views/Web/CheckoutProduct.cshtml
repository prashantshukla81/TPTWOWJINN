<script>
    function deletecheckout(Productid,custid,Srno)
    {
        var mm=confirm('Are you sure want to delete this Record?')
        if(mm==true)
        {
            var std = {};

            $.ajax({

                url: '@Url.Action("DeleteCheckoutproduct","Web")',
                method: "POST",
                data: {action:'6',srno:Productid,CustomerCode:custid},

                success: function () {
                    alert("Product Delete  successfully.");
                    window.location.reload(true);
                },
                error: function () {
                    alert("Error while inserting data");
                }
            });
        }
    }
