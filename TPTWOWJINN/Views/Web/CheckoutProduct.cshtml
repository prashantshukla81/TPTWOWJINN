    function Updatecart(obj,Type,Pid)
    {
        var x;//txtsalerate
        var qty=$(obj).parent('div').find('#txtqty').val();
        var salerate=$(obj).parent('div').find('#txtsalerate').text();
        if(Type=="A")
        {
            var x = parseInt(qty)+1;
            $(obj).parent('div').find('#txtqty').val(x);



        }
        if(Type=="S")
        {
            var x = parseInt(qty)-1;
            if (x <= 0) {
                return;
            }

            $(obj).parent('div').find('#txtqty').val(x);

        }


        var dataobject = {
            Action: '1',
            productid: Pid,
            RespoCode: @Session["UserName"],
            qty:x
        };

        $.ajax({
            url: "@Url.Action("Updatecart", "Customer")",
            method: "POST",
            contentType: false,
            processData: false,
            data: JSON.stringify(dataobject),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success:function(r)
            {
                alert(r.qty)
                $(obj).parent('div').find('#txtqty').val(r.qty);
                $(obj).parent('div').parent('div').find('#txtsalerate').text(r.Servicetype);
                alert(r.Servicetype);
            },
            error:function(r)
            {
                alert('Error');
            }

        })

    }

